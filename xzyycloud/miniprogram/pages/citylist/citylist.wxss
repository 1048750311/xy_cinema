page{
  background-color: #ebebeb;
}
.city-list-container{
  font-size: 14px;
  color: #333;
}
.city-title {
  padding-left: 15px;
  line-height: 30px;
}
.city-inline{
  background-color: #f5f5f5;
  padding: 20rpx;
}
.location-city {
  display: inline-block;
  width: auto;
  padding: 0 20px;
  background: #fff;
  height: 34px;
  line-height: 34px;
}
.city-list{
  height:100%;
  background: #fff;
  display: flex;
}
.city-left{
  flex: 1;
}
.city-right{
  position: fixed;
  top: 0;
  right: 0;
  width: 60rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.city-right .py-item{
  text-align: center;
  padding:4rpx 0;
}
.city-group-item{
  margin:10rpx 0;
}
.city-group-title{
  background-color:#ebebeb;
  padding: 10rpx;
}
.city-fullname{
  padding:15rpx 20rpx;
}